<h1>Edit Artwork: <%= @artwork.name %></h1>

<%= simple_form_for(@artwork) do |f| %>
  <%= f.input :name %>
  <%= f.input :artist %>
  <%= f.input :category %>
  <%= f.input :price %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :measurements %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Submit", class: "form-button" %>
<% end %>
